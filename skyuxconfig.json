{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-modals",
  "mode": "easy",
  "compileMode": "aot",
  "skyuxModules": [
    "SkyErrorModule"
  ],
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "pipelineSettings": {
    "vsts": {
      "testSettings": {
        "e2e": {
          "browserSet": false
        },
        "unit": {
          "browserSet": false
        }
      }
    }
  },
  "app": {
    "styles": [
      "@skyux/docs-tools/css/docs-tools.css"
    ],
    "theming": {
      "supportedThemes": [
        "default",
        "modern"
      ],
      "theme": "default"
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
